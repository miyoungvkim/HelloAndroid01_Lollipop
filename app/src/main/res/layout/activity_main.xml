<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_go_to_servce_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Go to Service Layout" />

    <Button
        android:id="@+id/btn_go_to_youtube_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Go To Youtube Layout" />

    <Button
        android:id="@+id/btn_go_to_tenserflow_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Go To Tenserflow Layout" />

</LinearLayout>